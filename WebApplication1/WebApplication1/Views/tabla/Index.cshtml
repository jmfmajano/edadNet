@model List<WebApplication1.Models.operaciones>
@{
    ViewBag.Title = "Tablas";
}

<div class="panel panel-primary">
    <div class="panel-heading">Tablas de multiplicar</div>
        <div class="panel-body">
            <form action="/" method="post">
                <div class="form-group">
                    <input type="text" name="txttabla" class="form-control" value="" autocomplete="off" placeholder="Digite numero de la tabla ha multiplicar."/>
                </div>
                <div class="form-group">
                    <input type="submit" class="btn btn-success btn-lg" name="name" value="CALCULAR"/>
                </div>
            </form>            
        </div> 
</div>

